type I1 where
  i1 : @int -> int -> I1

fun1 (i1 @i j) = addInt i j

type I2 where
  i2 : int -> @int -> I2

fun2 (i2 i @j) = addInt i j

type I3 where
  i3 : @int -> @int -> int -> int -> I3

fun3 (i3 @i @j k l) = addInt (addInt i j) (addInt k l)

type I4 where
  i4 : int -> int -> @int -> @int -> I4

fun4 (i4 i j @k @l) = addInt (addInt i j) (addInt k l)
