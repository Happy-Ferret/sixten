bool' : type
bool' = forall (a : type). a -> a -> a

True' : bool'
True' x y = x

False' : bool'
False' x y = y

if : forall (a : type). bool' -> a -> a -> a
if b = b

and : bool' -> bool' -> bool'
and b1 b2 = if b1 b2 False'

or : bool' -> bool' -> bool'
or b1 b2 = if b1 True' b2

main1 = printInt (if (or False' (and True' False')) 12 13)
