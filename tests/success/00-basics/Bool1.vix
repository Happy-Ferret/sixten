bool' : type
bool' = @(a : type) -> a -> a -> a

True' : bool'
True' x y = x

False' : bool'
False' x y = y

if : forall a. bool' -> a -> a -> a
if b = b

and : bool' -> bool' -> bool'
and b1 b2 = if b1 b2 False'

or : bool' -> bool' -> bool'
or b1 b2 = if b1 True' b2

main2 = printInt (if (or (and True' False') True') 12 13)
