deref : forall A. Ptr A -> A
deref p = case p of Ptr a -> a

type Unit' where
  unit : Unit'

type Tuple A B where
  tuple : A -> B -> Tuple A B

ntuple : (n : nat) -> type
ntuple n = case n of
  Zero -> Unit'
  Succ n' -> Tuple int (ntuple n')

test : ntuple (Succ (Succ Zero))
test = tuple 0 (tuple 1 unit)

lol = printInt case test of
  tuple 0 (tuple 1 unit) -> 111
  _ -> 222
